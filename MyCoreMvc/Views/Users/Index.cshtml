@model IEnumerable<User>

<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>Email</th>
            <th>EmailConfirmed</th>
            <th>PasswordHash</th>
            <th>PhoneNumber</th>
            <th>PhoneNumberConfirmed</th>
            <th>SecurityStamp</th>
            <th>TwoFactorEnabled</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var u in Model)
        {
            <tr>
                <td>@u.Id</td>
                <td>@u.UserName</td>
                <td>@u.Email</td>
                <td>@u.EmailConfirmed</td>
                <td>@u.PasswordHash</td>
                <td>@u.PhoneNumber</td>
                <td>@u.PhoneNumberConfirmed</td>
                <td>@u.SecurityStamp</td>
                <td>@u.TwoFactorEnabled</td>
                <td>
                    <form asp-action="Destroy" asp-route-id="@u.Id" method="post">
                        <button type="submit" class="btn btn-sm btn-danger">Delete</button>
                    </form>
                </td>
            </tr>
        }
    </tbody>
</table>
